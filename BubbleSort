import random
a = list()

arraySize = 10

for x in range(arraySize):
	a.append(random.randint(1,100))

swapped = True
while swapped: #just putting swapped is the same as swapped == True
	swapped = False
	for i in range(arraySize - 1):
		if a[i] > a[i+1]:
			temp = a[i]
			a[i] = a[i+1]
			a[i+1] = temp
			swapped = True

for x in range(arraySize):
	print(a[x])
